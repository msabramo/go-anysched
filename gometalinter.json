{
  "Vendor": true,
  "Deadline": "2m",
  "Sort": ["linter", "severity", "path", "line"],
  "Exclude": ["vendor"],
  "EnableGC": true,
  "Linters": {
      "gofmt": {
        "Command": "gofmt -l",
        "Pattern": "^(?P<path>.*?\\.go)$"
      },
      "nakedret": {
        "Command": "nakedret",
        "Pattern": "^(?P<path>.*?\\.go):(?P<line>\\d+)\\s*(?P<message>.*)$"
      },
      "printf": {
        "Command": "ag --noheading 'fmt\\.Printf'",
        "Pattern": "^(?P<path>.*?\\.go):(?P<line>\\d+)\\s*(?P<message>.*)$"
      }
  },
  "WarnUnmatchedDirective": true,

  "DisableAll": true,
  "Enable": [
    "errcheck",
    "gas",
    "gocyclo",
    "gofmt",
    "goimports",
    "golint",
    "interfacer",
    "lll",
    "nakedret",
    "vet"
  ],

  "Cyclo": 10,
  "LineLength": 120
}
